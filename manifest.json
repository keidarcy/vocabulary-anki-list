{
  "manifest_version": 3,
  "name": "Vocabulary Anki Memo",
  "description": "Quick English vocabulary search history anki memo",
  "version": "1.0.4",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["contextMenus", "storage"],
  "host_permissions": ["https://api.dictionaryapi.dev/"],
  "background": {
    "service_worker": "background.js"
  }
}
